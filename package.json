{
  "name": "nextjs14-template",
  "version": "1.0.0",
  "private": true,
  "author": {
    "name": "Marian",
    "email": "pidchashymaryan@gmail.com",
    "url": "https://github.com/Marian1309"
  },
  "scripts": {
    "commit": "git-cz",
    "check:updates": "ncu -i",
    "depcheck": "depcheck",
    "clean": "rimraf bun.lockb && rimraf .next && rm -rf node_modules",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "dev": "next dev",
    "build": "next build",
    "prod": "next build && next start",
    "prod:bundlecheck": "cross-env ANALYZE=true next build && next start",
    "format": "prettier . -w",
    "lint": "next lint --fix",
    "lint-staged": "lint-staged",
    "shadcn": "bunx --bun shadcn@latest add",
    "shadcn:all": "bunx --bun shadcn@latest add --all",
    "db:pull": "prisma db pull",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev --name",
    "db:gen": "prisma generate",
    "db:studio": "prisma studio",
    "test:unit": "vitest run --passWithNoTests",
    "test:e2e:all": "playwright test && rm -rf test-results",
    "test:e2e:chromium": "playwright test --project=chromium && rm -rf test-results",
    "test:e2e:firefox": "playwright test --project=firefox && rm -rf test-results",
    "test:e2e:safari": "playwright test --project=safari && rm -rf test-results",
    "test:e2e:list": "playwright test --list",
    "test:e2e:install": "playwright install-deps && playwright install"
  },
  "browserslist": {
    "production": [
      "> 0.1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  },
  "dependencies": {
    "@clerk/nextjs": "^6.9.6",
    "@hookform/resolvers": "^3.9.1",
    "@prisma/client": "5.21.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-icons": "1.3.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-slider": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.4",
    "@tanstack/react-query": "5.62.7",
    "axios": "1.7.9",
    "lucide-react": "0.468.0",
    "motion": "11.14.4",
    "next": "14.2.20",
    "next-themes": "0.4.4",
    "nuqs": "2.2.3",
    "react": "18.3.1",
    "react-countup": "6.5.3",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.54.2",
    "react-hot-toast": "2.4.1",
    "react-spinners": "0.15.0",
    "recharts": "^2.15.0",
    "sharp": "0.33.5",
    "sonner": "1.7.1",
    "svix": "^1.44.0",
    "uuid": "11.0.3",
    "zod": "^3.24.1",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "19.6.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@commitlint/format": "19.5.0",
    "@commitlint/types": "19.5.0",
    "@next/bundle-analyzer": "14.2.20",
    "@playwright/test": "^1.49.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/node": "22",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.7",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "10.4.20",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "commitizen": "^4.3.1",
